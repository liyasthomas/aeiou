{
	"entrypoint": "index.html",
	"shell": "src/my-app.js",
	"sources": [
		"images/**/*",
		"data/**/*",
		"gltf/**/*"
	],
	"extraDependencies": [
		"manifest.json",
		"node_modules/@webcomponents/webcomponentsjs/*.js",
		"!node_modules/@webcomponents/webcomponentsjs/gulpfile.js",
		"node_modules/@webcomponents/webcomponentsjs/bundles/*.js",
		"node_modules/web-animations-js/web-animations-next-lite.min.js",
		"node_modules/@fabricelements/skeleton-carousel/skeleton-carousel.js",
		"node_modules/@google/model-viewer/dist/model-viewer.js"
	],
	"builds": [
		{
			"name": "esm-bundled",
			"browserCapabilities": [
				"es2015",
				"modules"
			],
			"js": {
				"minify": true
			},
			"css": {
				"minify": true
			},
			"html": {
				"minify": true
			},
			"bundle": true,
			"addServiceWorker": true
		},
		{
			"name": "es6-bundled",
			"browserCapabilities": [
				"es2015"
			],
			"js": {
				"minify": true,
				"transformModulesToAmd": true
			},
			"css": {
				"minify": true
			},
			"html": {
				"minify": true
			},
			"bundle": true,
			"addServiceWorker": true
		},
		{
			"name": "es5-bundled",
			"js": {
				"minify": true,
				"compile": true,
				"transformModulesToAmd": true
			},
			"css": {
				"minify": true
			},
			"html": {
				"minify": true
			},
			"bundle": true,
			"addServiceWorker": true
		}
	],
	"moduleResolution": "node",
	"npm": true,
	"lint": {
		"rules": [
			"polymer-3"
		]
	}
}
